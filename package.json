{
  "name": "amrnet",
  "author": "Louise Cerdeira",
  "version": "1.0.2",
  "repository": "ehmpathy/declapract",
  "homepage": "https://github.com/amrnet/amrnet",
  "keywords": [
    "mern",
    "amr",
    "bacteria",
    "mongo",
    "javascript",
    "genomic",
    "metadata"
  ],
  "bugs": "https://github.com/amrnet/amrnet/issues",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn client\"",
    "install": "cd client && yarn",
    "client": "cd client && yarn start",
    "build": "cd client && yarn build"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.2",
    "cookie-parser": "~1.4.5",
    "cosmiconfig": "^9.0.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^10.0.0",
    "express": "~4.17.1",
    "lz-string": "^1.5.0",
    "mongodb": "^3.6.9",
    "mongoose": "^5.13.1",
    "nodemailer": "^6.6.2",
    "objects-to-csv": "^1.3.6",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.11",
    "nodemon": "^2.0.9",
    "if-env": "^1.0.4",
    "lint-staged": "^15.2.2",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "^14.15.0",
    "yarn": "^1.22.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
