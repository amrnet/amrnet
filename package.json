{
  "name": "amrnet",
  "author": "Louise Cerdeira",
  "version": "1.1.1",
  "description": "AMRnet server",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:backend": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"yarn client\"",
    "start:dev-script": "./start-dev.sh",
    "install": "cd client && npm install",
    "client": "cd client && npm run start:no-lint",
    "build": "cd client && yarn build",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "db:indexes": "node scripts/create-indexes.js",
    "lint:check": "cd client && npm run lint:check",
    "lint": "cd client && npm run lint",
    "lint:fix": "cd client && npm run lint:fix"
  },
