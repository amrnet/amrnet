{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    // ...
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"]
}
